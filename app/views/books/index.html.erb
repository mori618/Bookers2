<% @books.each do |book| %>
  <div>
    <%= link_to book_path(book.id) do %>
      <%= image_tag book.get_image %>
    <% end %>
    <p>ユーザー：<%= image_tag 'sample-author1.jpg' %></p>
    <p>題名：<%= book.title %></p>
    <p>感想：<%= book.body %></p>
    <p>ユーザーネーム：<%= book.user.name %></p>

  </div>
<% end %>