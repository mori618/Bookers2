<div class="container d-flex justify-content-center">
<%= render 'books/list', user: @current_user, new_book: @book %>

<% @users.each do |user| %>
  <div>
    <p>ユーザー：<%= image_tag user.get_profile_image(100,100) %></p>
    <p>ユーザーネーム：<%= user.name %></p>
    <p><%= link_to "Show", user_path(user.id) %></p>
  </div>
<% end %>
</div>